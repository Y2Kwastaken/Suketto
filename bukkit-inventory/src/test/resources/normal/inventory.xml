<?xml version="1.0" encoding="UTF-8"?>
<chest title="[Example](color=#fff-#000)" rows="3" columns="9"
       responder="sh.miles.inventory.ContainerResponder">

    <hgroups>
        <hgroup row="3">
            <items>
                <item item-type="DIRT">
                    <name>test</name>
                </item>
                <item item-type="STONE"/>
                <item item-type="DIAMOND_BLOCK"/>
                <item item-type="NETHERITE_BLOCK"/>
                <item item-type="NETHERITE_BLOCK"/>
                <item item-type="DIAMOND_BLOCK" column="7"/>
                <item item-type="STONE"/>
                <item item-type="DIRT"/>
            </items>
        </hgroup>
    </hgroups>

    <vgroups>
        <vgroup column="1">
            <items>
                <item item-type="DIRT"/>
                <item item-type="STONE"/>
                <!-- Throws Warning because this occupies hte same spot as the first element in hgroup row 3-->
                <item item-type="DIAMOND_BLOCK"/>
                <!-- Any Item Below this throws warning-->
                <item item-type="NETHERITE_BLOCK"/>
            </items>
        </vgroup>
    </vgroups>

    <items>
        <!-- Throws warning as this item occupies the same spot as DIRT in column 1-->
        <item item-type="STONE"/>
    </items>

</chest>
